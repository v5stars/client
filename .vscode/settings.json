{
  "dart.env": {
    "ENABLE_FLUTTER_DESKTOP": true
  },
  "files.associations": {
    "*.arb": "json"
  },
  "protoc": {
    "path": "protoc",
    "options": [
        "--proto_path=proto/",
        "--dart_out=lib/grpc/"
    ]
  },
  "files.exclude": {
    ".dart_tool": true,
    ".idea": true,
    "build": true,
    "fonts": true,
    "android": true,
    "ios": true,
    "linux": true,
    "macos": true,
    "windows": true,
  }
}
